#include <stdio.h>
#include<string.h>
int main() {
    char str1[50],str2[50];
    scanf("%s",str1);
    int i,n,j=0;
    n=strlen(str1);
    for(i=n-1;i>=0;i--)
    {
        str2[j]=str1[i];
        j++;
    }
    str2[j]='\0';
    i=strcmp(str1,str2);
    if(i==0)
    printf("palindrome");
    else
    printf("not aÂ palindrome");
}
